<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D6EAF8"
    tools:context=".Q10">

    <Button
        android:id="@+id/i1"
        android:layout_width="215dp"
        android:layout_height="56dp"
        android:layout_marginBottom="280dp"
        android:text="10 สายการบิน"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/i2"
        android:layout_width="212dp"
        android:layout_height="60dp"
        android:layout_marginBottom="208dp"
        android:text="5 สายการบิน"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/i3"
        android:layout_width="208dp"
        android:layout_height="57dp"
        android:layout_marginBottom="144dp"
        android:text="7 สายการบิน"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/i4"
        android:layout_width="205dp"
        android:layout_height="60dp"
        android:layout_marginBottom="76dp"
        android:text="8 สายการบิน"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="347dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:text="สายการบินที่บินตามกำหนดการของประเทศไทยในปัจจุบันมีทั้งหมดกี่สายการบิน ? "
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="223dp"
        android:layout_height="197dp"
        android:layout_marginTop="156dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/q10" />
</androidx.constraintlayout.widget.ConstraintLayout>